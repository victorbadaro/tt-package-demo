{
  "name": "tt-package-demo",
  "version": "0.0.1",
  "description": "A demo package built following the Total Typescript article",
  "keywords": [
    "demo",
    "typescript"
  ],
  "main": "dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "homepage": "https://github.com/victorbadaro/tt-package-demo",
  "bugs": {
    "url": "https://github.com/victorbadaro/tt-package-demo/issues"
  },
  "author": "Victor Badar√≥ <victor.badaro@fatec.sp.gov.br> (https://github.com/victorbadaro)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victorbadaro/tt-package-demo.git"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.4",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "scripts": {
    "build": "tsup",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run lint && npm run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack .",
    "lint": "tsc",
    "test": "vitest run",
    "dev": "vitest"
  }
}